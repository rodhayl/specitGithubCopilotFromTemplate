{
  "qoder": {
    "version": "1.0.0",
    "ai": {
      "enabled": true,
      "provider": "qoder-ai",
      "model": "qoder-latest",
      "completion": {
        "enabled": true,
        "contextLength": 5000,
        "multiline": true,
        "triggerDelay": 100,
        "quality": "high",
        "diversity": true,
        "cacheEnabled": true
      },
      "chat": {
        "enabled": true,
        "contextAware": true,
        "codebaseIntegration": true,
        "maxHistory": 50
      },
      "codeAnalysis": {
        "enabled": true,
        "deepAnalysis": true,
        "semanticUnderstanding": true,
        "crossFileAnalysis": true,
        "performanceOptimization": true
      }
    },
    "workflows": {
      "enabled": true,
      "directory": ".qoder/workflows",
      "autoTrigger": true,
      "triggers": {
        "fileChanges": [
          "src/**/*.js",
          "src/**/*.ts",
          "src/**/*.jsx",
          "src/**/*.tsx",
          "src/**/*.py",
          "src/**/*.java",
          "src/**/*.go",
          "src/**/*.rs"
        ],
        "gitEvents": ["commit", "push", "pull_request"],
        "timeBasedTriggers": true,
        "customTriggers": []
      }
    },
    "prompts": {
      "directory": ".qoder/prompts",
      "autoLoad": true,
      "customPrompts": true,
      "promptTemplates": true
    },
    "agents": {
      "enabled": true,
      "maxConcurrent": 3,
      "specialization": {
        "codeGeneration": true,
        "codeReview": true,
        "testing": true,
        "documentation": true,
        "debugging": true
      }
    }
  },
  "editor": {
    "fontSize": 14,
    "fontFamily": "'Fira Code', 'JetBrains Mono', 'Cascadia Code', Consolas, monospace",
    "fontLigatures": true,
    "lineHeight": 1.5,
    "tabSize": 2,
    "insertSpaces": true,
    "detectIndentation": true,
    "wordWrap": "on",
    "minimap": {
      "enabled": true,
      "maxColumn": 120
    },
    "rulers": [80, 120],
    "formatOnSave": true,
    "formatOnPaste": true,
    "codeActions": {
      "onSave": {
        "fixAll": true,
        "organizeImports": true,
        "addMissingImports": true
      }
    },
    "inlineSuggest": {
      "enabled": true,
      "showToolbar": "onHover"
    },
    "suggest": {
      "preview": true,
      "showInlineDetails": true
    },
    "quickSuggestions": {
      "other": "on",
      "comments": "on",
      "strings": "on"
    }
  },
  "files": {
    "autoSave": "afterDelay",
    "autoSaveDelay": 1000,
    "trimTrailingWhitespace": true,
    "insertFinalNewline": true,
    "exclude": {
      "**/node_modules": true,
      "**/dist": true,
      "**/build": true,
      "**/.git": true,
      "**/.DS_Store": true,
      "**/coverage": true,
      "**/.next": true,
      "**/.nuxt": true,
      "**/out": true,
      "**/.qoder/cache": true
    }
  },
  "workbench": {
    "colorTheme": "Qoder Dark",
    "iconTheme": "qoder-icons",
    "editor": {
      "enablePreview": false,
      "closeOnFileDelete": true
    },
    "startupEditor": "welcomePage"
  },
  "terminal": {
    "integrated": {
      "fontSize": 13,
      "fontFamily": "'Fira Code', 'JetBrains Mono', Consolas, monospace",
      "defaultProfile": {
        "windows": "PowerShell",
        "osx": "zsh",
        "linux": "bash"
      }
    }
  },
  "git": {
    "enableSmartCommit": true,
    "confirmSync": false,
    "autofetch": true,
    "showPushSuccessNotification": true,
    "suggestSmartCommit": true
  },
  "languages": {
    "typescript": {
      "preferences": {
        "quoteStyle": "single"
      },
      "suggest": {
        "autoImports": true
      },
      "updateImportsOnFileMove": "always",
      "inlayHints": {
        "parameterNames": "all",
        "variableTypes": true,
        "functionLikeReturnTypes": true
      }
    },
    "javascript": {
      "preferences": {
        "quoteStyle": "single"
      },
      "suggest": {
        "autoImports": true
      },
      "updateImportsOnFileMove": "always",
      "inlayHints": {
        "parameterNames": "all",
        "variableTypes": true,
        "functionLikeReturnTypes": true
      }
    }
  },
  "linting": {
    "eslint": {
      "validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "vue",
        "html",
        "markdown"
      ],
      "format": {
        "enable": true
      }
    }
  },
  "formatting": {
    "prettier": {
      "singleQuote": true,
      "trailingComma": "es5",
      "tabWidth": 2,
      "semi": true,
      "printWidth": 100,
      "bracketSpacing": true,
      "arrowParens": "avoid"
    }
  },
  "search": {
    "exclude": {
      "**/node_modules": true,
      "**/dist": true,
      "**/build": true,
      "**/coverage": true,
      "**/.git": true,
      "**/.qoder/cache": true
    }
  },
  "extensions": {
    "autoUpdate": true,
    "ignoreRecommendations": false
  },
  "performance": {
    "indexing": {
      "incremental": true,
      "background": true
    },
    "caching": {
      "enabled": true,
      "strategy": "intelligent"
    },
    "memoryOptimization": true
  },
  "integrations": {
    "github": {
      "enabled": true,
      "autoSync": true
    },
    "docker": {
      "enabled": true
    },
    "testing": {
      "frameworks": ["jest", "mocha", "cypress", "playwright"],
      "autoRun": false,
      "coverage": true
    }
  }
}